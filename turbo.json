{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": [
				"**/*",
				"!test/**/*",
				"!.astro/**/*",
				"!.cache/**/*",
				"!.svelte-kit/**/*"
			],
			"outputs": [".svelte-kit/**", ".vercel/**", "dist/**"]
		},
		"typecheck": {},
		"lint": {
			"dependsOn": ["typecheck"]
		},
		"publish": {
			"dependsOn": ["build", "lint", "typecheck"],
			"cache": false
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
